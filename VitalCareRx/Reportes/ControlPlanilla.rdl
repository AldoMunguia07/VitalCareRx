  <DataSets>
    <DataSet Name="Anio">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select distinct(YEAR(CE.fechaEntrada)) 'Anio'
	from Historial.ControlEmpleado CE</CommandText>
      </Query>
      <Fields>
        <Field Name="Anio">
          <DataField>Anio</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ControlPlanilla">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Empleado">
            <Value>=Parameters!empleado.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Mes">
            <Value>=Parameters!mes.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Año">
            <Value>=Parameters!Año.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select DAY(CE.fechaEntrada) 'Dia del Mes', CONVERT(char(5),CE.fechaEntrada,108) 'Hora de Entrada', 
	convert(char(5), CE.fechaSalida,108) 'Hora Salida'
	from Historial.ControlEmpleado CE JOIN Personas.Empleado E
	ON CE.idEmpleado = E.idEmpleado
	WHERE CE.idEmpleado = @Empleado and MONTH(CE.fechaEntrada) = @Mes and YEAR(CE.fechaEntrada) = @Año</CommandText>
      </Query>
      <Fields>
        <Field Name="Dia_del_Mes">
          <DataField>Dia del Mes</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Hora_de_Entrada">
          <DataField>Hora de Entrada</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Hora_Salida">
          <DataField>Hora Salida</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Empleado">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select E.idEmpleado 'ID', CONCAT(E.primerNombre, ' ', E.primerApellido) 'Nombre del Empleado' from Personas.Empleado E</CommandText>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Nombre_del_Empleado">
          <DataField>Nombre del Empleado</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Mes">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SET LANGUAGE SPANISH
	select distinct(DATENAME(MONTH, CE.fechaEntrada)) 'Nombre del Mes', MONTH(CE.fechaEntrada) 'Numero de Mes' 
	from Historial.ControlEmpleado CE
	ORDER BY MONTH(CE.fechaEntrada)</CommandText>
      </Query>
      <Fields>
        <Field Name="Nombre_del_Mes">
          <DataField>Nombre del Mes</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Numero_de_Mes">
          <DataField>Numero de Mes</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
